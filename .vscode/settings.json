    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "node_modules": false,
    "public": false,
    "package.json": false,
    "package-lock.json": false,
    ".gitignore": false,
    ".vscode": false
  },
}